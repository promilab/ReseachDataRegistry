{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["title", "authors", "year", "keywords", "url"],
    "properties": {
      "title": { "type": "string" },
      "authors": { "type": "string" },
      "year": { "type": "integer" },
      "keywords": { "type": "string" },
      "url": { "type": "string", "format": "uri" }
    },
    "additionalProperties": false
  }
}
